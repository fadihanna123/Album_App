{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext", "DOM.Iterable"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext",
    "sourceMap": false
  },
  "include": ["src/**/*", "App.tsx"],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
