{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": ["dom", "esnext", "DOM.Iterable"],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "target": "ESNext"
  },
  "include": ["src/**/*", "App.tsx", "index.d.ts"],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
